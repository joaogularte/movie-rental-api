{
    "extends": "airbnb",
    "globals": {
        "describe": true,
        "beforeEach": true,
        "afterEach": true,
        "expect": true,
        "it": true,
        "request": true
    },
    "rules": {
        "prefer-destructuring": ["error", {
            "object": false
          }],
        "no-console": ["error", {"allow": ["log"]}]
    }
}